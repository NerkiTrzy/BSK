<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="768" minWidth="1024" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="police.login.ChangePasswordController">
   <children>
      <JFXPasswordField fx:id="oldPassword" layoutX="433.0" layoutY="201.0" prefHeight="25.0" prefWidth="159.0" promptText="Stare hasło" />
      <JFXPasswordField fx:id="newPassword" layoutX="433.0" layoutY="231.0" prefHeight="25.0" prefWidth="159.0" promptText="Nowe hasło" />
      <JFXPasswordField fx:id="repeatPassword" layoutX="433.0" layoutY="261.0" prefHeight="25.0" prefWidth="159.0" promptText="Powtórz hasło" />
      <JFXButton fx:id="changePasswordButton" buttonType="RAISED" layoutX="432.0" layoutY="318.0" mnemonicParsing="false" onAction="#changePassword" prefHeight="25.0" prefWidth="159.0" style="-fx-background-color: #1e88e5;" text="Zmień hasło" textFill="WHITE" />
      <JFXButton fx:id="cancelButton" buttonType="RAISED" layoutX="432.0" layoutY="368.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="25.0" prefWidth="159.0" style="-fx-background-color: #1e88e5;" text="Anuluj" textFill="WHITE" />

   </children>

</AnchorPane>
