<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXPasswordField?>
<AnchorPane minHeight="300" minWidth="400" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="police.admin.newbie.NewbieController">
   <children>
      <Button fx:id="cancelButton" layoutX="250.0" layoutY="270.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="100.0" text="Anuluj" />
      <Button fx:id="confirmButton" layoutX="50.0" layoutY="270.0" mnemonicParsing="false" onAction="#confirm" prefHeight="25.0" prefWidth="100.0" text="Zatwierdź" />
      <JFXTextField fx:id="loginTextField" layoutX="50.0" layoutY="70.0" prefHeight="25.0" prefWidth="159.0" promptText="Login" />
      <JFXPasswordField fx:id="passwordTextField" layoutX="50.0" layoutY="120.0" prefHeight="25.0" prefWidth="159.0" promptText="Hasło" />
      <JFXPasswordField fx:id="passwordRepeatTextField" layoutX="50.0" layoutY="170.0" prefHeight="25.0" prefWidth="159.0" promptText="Ponów hasło" />
      <TextField fx:id="labelTextField" layoutX="50.0" layoutY="220.0" prefHeight="25.0" prefWidth="105.0" />
   </children>

</AnchorPane>
