<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane minHeight="768" minWidth="1024" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="police.admin.AdminController">
   <children>
      <Button fx:id="backButton" layoutX="868.0" layoutY="729.0" mnemonicParsing="false" onAction="#backToMainMenu" prefHeight="25.0" prefWidth="146.0" text="Cofnij" />
       <Button fx:id="saveButton" layoutX="90.0" layoutY="729.0" mnemonicParsing="false" onAction="#saveGrid" prefHeight="25.0" prefWidth="146.0" text="Zapisz zmiany" />
      <TableView fx:id="userTableView" layoutX="90.0" layoutY="25.0" prefHeight="510.0" prefWidth="769.0">
        <columns>
           <TableColumn fx:id="userNameColumn" prefWidth="300.0" text="Nazwa użytkownika">
              <cellValueFactory>
                 <PropertyValueFactory property="userName" />
              </cellValueFactory>
           </TableColumn>
           <TableColumn fx:id="valueColumn" prefWidth="120.0" text="Wartość etykiety">
              <!--<cellValueFactory>
                 <PropertyValueFactory property="value" />
              </cellValueFactory>-->
           </TableColumn>
           <TableColumn fx:id="roleNameColumn" prefWidth="220.0" text="Nazwa etykiety">
              <!-- <cellValueFactory>
                  <PropertyValueFactory property="roleName" />
               </cellValueFactory>-->
            </TableColumn>
         </columns>
       </TableView>
       <Button fx:id="newUserButton" onAction="#addNewUser" layoutX="874.0" layoutY="25.0" mnemonicParsing="false" prefHeight="64.0" prefWidth="138.0" text="Nowy użytkownik" />
       <Button fx:id="deleteUserButton" onAction="#deleteUser" layoutX="874.0" layoutY="100.0" mnemonicParsing="false" prefHeight="64.0" prefWidth="138.0" text="Usuń użytkownika" />
    </children>

 </AnchorPane>
