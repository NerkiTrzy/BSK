<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="768" minWidth="1024" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="police.Controller">
<children>

    <Button fx:id="policemanButton" layoutX="48.0" layoutY="201.0" onAction="#showPolicemanPanel" prefHeight="96.0" prefWidth="171.0">Panel Policjanta</Button>
    <Button fx:id="dispatcherButton" layoutX="45.0" layoutY="336.0" onAction="#showDispatcherPanel" prefHeight="96.0" prefWidth="171.0">Panel Dystrybutora</Button>
    <Button fx:id="accountantButton" layoutX="45.0" layoutY="474.0" onAction="#showAccountantPanel" prefHeight="96.0" prefWidth="171.0">Panel Księgowego</Button>
    <Button fx:id="commanderButton" layoutX="45.0" layoutY="600.0" onAction="#showCommanderPanel" prefHeight="96.0" prefWidth="171.0">Panel Komendanta</Button>
    <Button fx:id="newsButton" layoutX="48.0" layoutY="62.0" onAction="#showNewsPanel" prefHeight="96.0" prefWidth="171.0" text="Nowości" />
    <Button fx:id="adminButton" layoutX="785.0" layoutY="211.0" onAction="#showAdminPanel" prefHeight="96.0" prefWidth="171.0" text="Panel Admina">Panel Admina</Button>
    <Button fx:id="logoutButton" onAction="#logout" layoutX="871.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Wyloguj" />
    <Button fx:id="changePasswordButton" onAction="#changePassword" layoutX="871.0" layoutY="44.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Zmień hasło" />
</children>

</AnchorPane>
